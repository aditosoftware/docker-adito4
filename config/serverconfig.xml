                                    </node>
                                </map>
                            </node>
                            <node name="system">
                                <map>
                                    <entry key="server id" value="{SRVCONF_SERVER_ID}"/>
                                </map>
                            </node>
                        </node>
                    </node>
                </node>
            </node>
        </node>
    </root>
