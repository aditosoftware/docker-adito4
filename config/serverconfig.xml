<?xml version="1.0" encoding="UTF-8"?>

<preferences EXTERNAL_XML_VERSION="1.0">
    <root type="system">
        <map/>
        <node name="de">
            <map/>
            <node name="adito">
                <map/>
                <node name="aditoweb">
                    <map/>
                    <node name="server">
                        <map/>
                        <node name="prefs">
                            <map>
                                <entry key="version" value="1.1.1"/>
                            </map>
                            <node name="systemdb">
                                <map>
                                    <entry key="caching" value="3"/>
